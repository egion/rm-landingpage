    <script type="text/javascript">
		var time =0;
		var timer;
		function setTime(){
			time = time +1;
			
		}
		function eingaben_ueberpruefen(){
			 clearInterval(timer);
			 var mail = document.Formular.email.value;
			 if (document.Formular.name.value.length < 3) { // Vorname mindestens 2 Zeichen
			  document.Formular.name.placeholder = "Geben Sie Ihren Namen ein";
			  document.Formular.name.focus();
			  return false;
			 }

			 
			 else if (mail.length < 10 || mail.indexOf ('@',0) == -1 || mail.indexOf ('.',0) == -1) {
			  
			  document.Formular.email.select();
			  return false;
			 }
			 else if (document.Formular.message.value.length < 10) { // Nachricht mindestens 19 Zeichen
			  document.Formular.email.placeholder = "Geben Sie Ihre Nachricht ein";
			  document.Formular.message.focus();
			  return false;
			 }
			  else if (document.Formular.dataprotection.checked === false) { // Nachricht mindestens 19 Zeichen
			  
			  document.Formular.dataprotection.focus();
			  return false;
			 }
			 else if (time < 4) { // Botschutz
			  
			  document.Formular.name.focus();
			  return false;
			 }
			 else
			 return true;
		}
		function startTimer(){
			timer =setInterval(setTime,1000);
		}
		
		</script>
